
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<h3>@ViewBag.Message</h3>

<p>
    There are @ViewBag.CustomerCount customers in the system. 
</p>

<ul>
    @foreach(var item in ViewBag.CustomerList)
    {
        <li>
            @item.Id -- @item.Name -- @item.Amount
        </li>
    }
    
</ul>